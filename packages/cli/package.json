{
  "name": "@motifxd/cli",
  "version": "0.0.0",
  "description": "motifXD CLI for working with the motif ecosystem inside your terminal",
  "author": {
    "name": "jimmy and walee",
    "email": "founders@labxd.com",
    "url": "https://labxd.com"
  },
  "bin": {
    "motif": "./bin/run"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "motif-data.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.7",
    "eslint-config-custom": "workspace:*",
    "ink": "^3.2.0",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-select-input": "^4.2.1",
    "react": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/ink-big-text": "^1.2.1",
    "@types/ink-gradient": "^2.0.1",
    "@types/node": "^18",
    "@types/react": "^18.0.25",
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "nodemon": "^2.0.20",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "motif",
    "dirname": "motif",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev:run": "./bin/dev $(< .args)",
    "dev": "nodemon --watch ./src --watch ./.args -e ts,tsx --exec \"pnpm run dev:run\"",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/https://github.com/HouseOfXD/motifXD/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/HouseOfXD/motifXD.git"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
